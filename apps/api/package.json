    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongoose": "^8.11.0",
    "tsx": "^4.19.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.12.8",
    "cross-env": "^7.0.3",
    "mongodb-memory-server": "^9.3.0",
    "nodemon": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.4"
